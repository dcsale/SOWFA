FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      turbineArrayProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
 
globalProperties
{
    outputControl       "timeStep";
    outputInterval       1;
}


// NacYaw:
// This is the initial or fixed nacelle yaw angle. It is positive counterclockwise when looking down
// on the turbine (rotate about z-axis). This value must be greater than â€“180 and less than or equal to 180 degrees.
turbine1
{
    turbineType         "DOE-Tidal-Ref-Model_FullScale";
    baseLocation        (100.0 110.0 0.0);   // no account for OverHang distance
    numBladePoints      30;
    pointDistType       "uniform";
    pointInterpType     "linear";
    bladeUpdateType     "oldPosition";
    epsilon             4.0;
    tipRootLossCorrType "Glauert";
    rotationDir         "ccw";
    Azimuth             0;
    TorqueGen                                                9999.9;
    RotSpeed            11.5;
    Pitch               0;
    NacYaw              -90;
    fluidDensity        1025.0;
}

turbine2
{
    turbineType         "DOE-Tidal-Ref-Model_FullScale";
    baseLocation        (100.0 138.0 0.0);   // no account for OverHang distance
    numBladePoints      30;
    pointDistType       "uniform";
    pointInterpType     "linear";
    bladeUpdateType     "oldPosition";
    epsilon             4.0;    
    tipRootLossCorrType "Glauert";
    rotationDir         "cw";
    Azimuth             90;
    TorqueGen                                                9999.9;
    RotSpeed            11.5;    
    Pitch               0;
    NacYaw              -90;
    fluidDensity        1025.0;
}
